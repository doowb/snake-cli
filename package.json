{
  "name": "snake-cli",
  "description": "Experiment / demo using readline-utils to create a simple snake game in the terminal.",
  "version": "0.1.1",
  "homepage": "https://github.com/doowb/snake-cli",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/snake-cli",
  "bugs": {
    "url": "https://github.com/doowb/snake-cli/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js"
  ],
  "preferGlobal": true,
  "bin": {
    "snake": "cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "readline-utils": "^0.1.2",
    "repeat-string": "^1.5.4",
    "window-size": "^0.2.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.1.2"
  },
  "keywords": [
    "cli",
    "snake"
  ],
  "verb": {
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "enquirer",
        "readline-ui",
        "readline-utils",
        "repeat-string",
        "window-size"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
